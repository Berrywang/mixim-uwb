# makefrag for "core" directory of the Mobility Framework
#
# this file automatically gets inserted into Makefiles generated by opp_makemake

# preserve the default target!
all: $(TARGET)


# all _m.h files must exist before we build anything, so run opp_msgc first
subdirs $(OBJS) : generateheaders

# add dependencies
include_dir: basicMessages_dir  basicModules_dir  blackboard_dir  channelControl_dir  utils_dir
lib_dir: include_dir

